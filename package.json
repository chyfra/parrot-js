{
  "name": "@chyfra/parrot-js",
  "version": "1.0.3-beta.1",
  "main": "index.js",
  "repository": {
    "url": "git+https://github.com/chyfra/parrot-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": "./dist/installer.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chyfra/parrot-js/issues"
  },
  "homepage": "https://github.com/chyfra/parrot-js#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build:types": "tsc --build ./tsconfig.build.json",
    "build:prod": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config webpack.config.ts --mode production",
    "build": "npm run clean && npm run build:types && npm run build:prod",
    "start": "node ./dist/index.js",
    "dev:watch": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config webpack.config.ts --watch",
    "dev": "nodemon",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "husky",
    "prepack": "chmod +x dist/installer.js && chmod +x dist/index.js"
  },
  "keywords": [],
  "author": "",
  "description": "",
  "dependencies": {
    "axios": "^1.7.7",
    "blessed": "^0.1.81",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "fs-extra": "^11.2.0",
    "graph-fs": "^1.1.1",
    "http": "^0.0.1-security",
    "http-proxy-agent": "^7.0.2",
    "https": "^1.0.0",
    "kleur": "^4.1.5",
    "nanoid": "^5.0.9",
    "prompts": "^2.4.2",
    "selfsigned": "^2.4.1",
    "winston": "^3.16.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/blessed": "^0.1.25",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.8.4",
    "@types/prompts": "^2.4.9",
    "@types/selfsigned": "^2.0.4",
    "@types/terminal-kit": "^2.5.6",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.1",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "git-format-staged": "^3.1.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
